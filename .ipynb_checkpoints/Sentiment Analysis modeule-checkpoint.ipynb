   "source": [
    "from textblob import TextBlob \n",
    "import re\n",
    "\n",
    "def cleaner_ (sent):\n",
    "    return ' '.join(re.sub(\"(@[A-Za-z0-9]+)|([^0-9A-Za-z \\t])|(\\w+:\\/\\/\\S+)\", \" \", sent).split())\n",
    "\n",
    "def sentAnalysis (sent):\n",
    "    return TextBlob(cleaner_(sent)).sentiment.polarity"
   ]
